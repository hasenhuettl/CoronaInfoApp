<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fhj.ima.com.example.coronainfoapp">


    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoronaInfoApp">

        <activity
            android:name="fhj.ima.com.example.coronainfoapp.Beschraenkungen"
            android:parentActivityName="fhj.ima.com.example.coronainfoapp.MainActivity">
        </activity>

        <activity
            android:name="fhj.ima.com.example.coronainfoapp.AktuelleZahlen"
            android:parentActivityName="fhj.ima.com.example.coronainfoapp.MainActivity">
        </activity>

        <activity
            android:name="fhj.ima.com.example.coronainfoapp.Ampelfarben"
            android:parentActivityName="fhj.ima.com.example.coronainfoapp.MainActivity">
        </activity>

        <activity android:name="fhj.ima.com.example.coronainfoapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>